{
  "name": "arraypress/wp-register-post-type",
  "version": "1.0.0",
  "description": "Lightweight library for registering custom post types in WordPress with smart defaults, automatic label generation, and permalink management.",
  "homepage": "https://github.com/arraypress/wp-register-post-type/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\RegisterPostType\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-post-type/issues",
    "source": "https://github.com/arraypress/wp-register-post-type"
  },
  "keywords": [
    "wordpress",
    "post-type",
    "cpt",
    "custom-post-type",
    "register",
    "permalink",
    "rewrite",
    "labels",
    "admin",
    "content-types"
  ]
}